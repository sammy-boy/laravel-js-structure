{
    "name": "anzarke/laravel-js-structure",
    "description": "Laravel package for setting up structured JavaScript services and files.",
    "autoload": {
        "psr-4": {
            "AnzarKe\\LaravelJsStructure\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AnzarKe\\LaravelJsStructure\\ModulusServiceProvider"
            ]
        }
    },

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sammy-boy/laravel-js-structure.git"
        }
    ],
    "require": {
        "anzarke/laravel-js-structure": "dev-main"
    }
}
